{
  "name": "easy-auth-js-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && zip -j dist/deploy.zip dist/easy-auth-js-sample/browser/*",
